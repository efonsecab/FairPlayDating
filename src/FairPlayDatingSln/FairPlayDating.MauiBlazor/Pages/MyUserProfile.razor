<OverlaySpinner ShowSpinner=IsLoading></OverlaySpinner>
@if (MyUserProfileModel != null)
{
    <EditForm Model="this.MyUserProfileModel" OnValidSubmit="UpdateMyUserProfileAsync">
        <DataAnnotationsValidator></DataAnnotationsValidator>
        <ValidationSummary></ValidationSummary>
        <div class="myuserprofile-root-grid">
            <div class="myuserprofile-title">
                <h1>My User Profile</h1>
            </div>
            <div class="myuserprofile-content">
                <div class="about-section">
                    <div class="about-section-title">
                        <h2>About</h2>
                    </div>
                    <div class="about-section-content">
                        <InputTextArea class="form-control" @bind-Value="this.MyUserProfileModel.About" rows="5"></InputTextArea>
                    </div>
                </div>
                <div class="profilephoto-section">
                    <div class="profilephoto-section-title">
                        <h2>Profile Photo</h2>
                    </div>
                    <div class="profilephoto-section-content">
                        @if (this.ProfileUserPhotoModel != null)
                        {
                            <img class="img-thumbnail" src="@ProfileUserPhotoModel.PhotoBlobUrl" />
                        }
                    </div>
                </div>
                @if (this.AllHairColors != null)
                {
                    <div class="haircolor-section">
                        <div class="haircolor-section-title">
                            <h2>Hair Color</h2>
                        </div>
                        <div class="haircolor-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.HairColorId">
                                <option value="">Please select your Hair Color</option>
                                @foreach (var singleHaircolor in this.AllHairColors)
                                {
                            <option value="@singleHaircolor.HairColorId">@singleHaircolor.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
                @if (this.AllHairColors != null)
                {
                    <div class="preferredhaircolor-section">
                        <div class="preferredhaircolor-section-title">
                            <h2>Preferred Hair Color</h2>
                        </div>
                        <div class="preferredhaircolor-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.PreferredHairColorId">
                                <option value="">Please select your Preferrd Hair Color</option>
                                @foreach (var singleHaircolor in this.AllHairColors)
                                {
                            <option value="@singleHaircolor.HairColorId">@singleHaircolor.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
                @if (this.AllEyesColors != null)
                {
                    <div class="eyescolor-section">
                        <div class="eyescolor-section-title">
                            <h2>Eyes Color</h2>
                        </div>
                        <div class="eyescolor-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.EyesColorId">
                                <option value="">Please select your Eyes Color</option>
                                @foreach (var singleEyesColor in this.AllEyesColors)
                                {
                            <option value="@singleEyesColor.EyesColorId">@singleEyesColor.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
                @if (this.AllEyesColors != null)
                {
                    <div class="preferredeyescolor-section">
                        <div class="preferredeyescolor-section-title">
                            <h2>Preferred Eyes Color</h2>
                        </div>
                        <div class="preferredeyescolor-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.PreferredEyesColorId">
                                <option value="">Please select your Preferred Eyes Color</option>
                                @foreach (var singleEyesColor in this.AllEyesColors)
                                {
                            <option value="@singleEyesColor.EyesColorId">@singleEyesColor.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
                @if (this.AllGenders != null)
                {
                    <div class="gender-section">
                        <div class="gender-section-title">
                            <h2>Biological Sex/Gender</h2>
                        </div>
                        <div class="gender-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.BiologicalGenderId">
                                <option value="">Please select your Biological Sex/Gender</option>
                                @foreach (var singleGender in this.AllGenders)
                                {
                            <option value="@singleGender.GenderId">@singleGender.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
                @if (this.AllDateObjectives != null)
                {
                    <div class="dateobjective-section">
                        <div class="dateobjective-section-title">
                            <h2>Date Objective</h2>
                        </div>
                        <div class="dateobjective-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.CurrentDateObjectiveId">
                                <option value="">Please select your Date Objective</option>
                                @foreach (var singleDateObjective in this.AllDateObjectives)
                                {
                            <option value="@singleDateObjective.DateObjectiveId">@singleDateObjective.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
                @if (this.AllReligions != null)
                {
                    <div class="religion-section">
                        <div class="religion-section-title">
                            <h2>Religion</h2>
                        </div>
                        <div class="religion-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.ReligionId">
                                <option value="">Please select your Religion</option>
                                @foreach (var singleReligion in this.AllReligions)
                                {
                            <option value="@singleReligion.ReligionId">@singleReligion.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
                @if (this.AllReligions != null)
                {
                    <div class="preferredreligion-section">
                        <div class="preferredreligion-section-title">
                            <h2>Preferred Religion</h2>
                        </div>
                        <div class="preferredreligion-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.PreferredReligionId">
                                <option value="">Please select your Preferred Religion</option>
                                @foreach (var singleReligion in this.AllReligions)
                                {
                            <option value="@singleReligion.ReligionId">@singleReligion.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
                @if (this.AllKidStatus != null)
                {
                    <div class="kidstatus-section">
                        <div class="kidstatus-section-title">
                            <h2>Kid Status</h2>
                        </div>
                        <div class="kidstatus-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.KidStatusId">
                                <option value="">Please select your Kids Status</option>
                                @foreach (var singleKidStatus in this.AllKidStatus)
                                {
                            <option value="@singleKidStatus.KidStatusId">@singleKidStatus.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
                @if (this.AllKidStatus != null)
                {
                    <div class="preferredkidstatus-section">
                        <div class="preferredkidstatus-section-title">
                            <h2>Preferred Kid Status</h2>
                        </div>
                        <div class="preferredkidstatus-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.PreferredKidStatusId">
                                <option value="">Please select your Preferred Kids Status</option>
                                @foreach (var singleKidStatus in this.AllKidStatus)
                                {
                            <option value="@singleKidStatus.KidStatusId">@singleKidStatus.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
                @if (this.AllTatooStatus != null)
                {
                    <div class="tatoostatus-section">
                        <div class="tatoostatus-section-title">
                            <h2>Tatoo Status</h2>
                        </div>
                        <div class="tatoostatus-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.TattooStatusId">
                                <option value="">Please select your Tatoo Status</option>
                                @foreach (var singleTatooStatus in this.AllTatooStatus)
                                {
                            <option value="@singleTatooStatus.TattooStatusId">@singleTatooStatus.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
                @if (this.AllTatooStatus != null)
                {
                    <div class="preferredtatoostatus-section">
                        <div class="preferredtatoostatus-section-title">
                            <h2>PreferredTatoo Status</h2>
                        </div>
                        <div class="preferredtatoostatus-section-content">
                            <InputSelect class="form-select" @bind-Value="MyUserProfileModel.PreferredTattooStatusId">
                                <option value="">Please select your Preferred Tatoo Status</option>
                                @foreach (var singleTatooStatus in this.AllTatooStatus)
                                {
                            <option value="@singleTatooStatus.TattooStatusId">@singleTatooStatus.Name</option>
                                }
                    </InputSelect>
                </div>
            </div>
                }
            </div>
            <div class="myuserprofile-footer">
                <div class="myuserprofile-footer-content">
                    <button type="submit" class="btn btn-primary">Save</button>
                </div>
            </div>
        </div>
    </EditForm>
}
<button type="button" @onclick="ShowFileDialogAsync">Load File</button>